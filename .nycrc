{
  "include": [
    "src/**/*",
    "lib/cjs/**/*"
  ],
  "exclude": [
    "src/test/**/*",
    "lib/cjs/test/**/*",
    "lib/module/**/*",
    "**/*.d.ts",
    "**/*.d.tsx"
  ],
  "temp-dir": "./lib/cjs/test/coverage/.nyc_output",
  "report-dir": "./lib/cjs/test/coverage",
  "reporter": [
    "text-summary",
    "lcov",
    "cobertura"
  ],
  "cache": false,
  "useSpawnWrap": true
}
